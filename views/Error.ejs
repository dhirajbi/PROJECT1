<% layout("/layouts/boilerplate") %>

<div class="row mt-3">
    <div class="alert alert-danger col-6 offset-3" role="alert">
        <h4 class="alert-heading"> <%= err.message %> </h4>

        <% if (err.stack) { %>
            <pre><%= err.stack %></pre>
        <% } %>

        <a href="/listings" class="btn btn-outline-dark mt-3">Go back to Listings</a>
    </div>
</div>
